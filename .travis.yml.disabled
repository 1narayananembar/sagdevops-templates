sudo: required

services:
  - docker

env:
  global:
    - COMPOSE_FILE=test.yml
  matrix:
    - CC_TEMPLATE=jenkins

before_install:
  - docker login -u "$DOCKER_USERNAME" -p "$DOCKER_PASSWORD"
  - docker-compose pull

script:
  - docker-compose run --rm init1
  - docker-compose run --rm test

