<project xmlns:au="antlib:org.apache.ant.antunit" >
    <import file="../../build.xml" />

  <target name="suiteSetUp" depends="sagenvInit2">
  </target>
    
	<target name="test-cc-repos-product" depends="sagenvInit2">
		<echo>${repo.product}</echo>
		<cc command="list repository products" expectedvalues="${repo.product}" format="csv" wait="0" />
	</target>

	<target name="test-cc-repos-fix" depends="sagenvInit2">
		<echo>${repo.fix}</echo>
		<cc command="list repository fixes" expectedvalues="${repo.fix}" format="csv" wait="0" />
	</target>

</project>